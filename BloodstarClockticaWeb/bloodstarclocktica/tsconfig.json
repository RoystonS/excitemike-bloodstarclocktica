{
	"compilerOptions": {
    "downlevelIteration": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"target": "es5",
		"declaration": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"lib": [
			"dom",
			"es2015"
		],
		"experimentalDecorators": true
	},

	"include": ["src/*"]
}
